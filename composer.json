{
    "name": "neoan3/neoan3",
    "homepage": "https://neoan3.rocks",
    "description": "official neoan3 cli",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "bin": [
        "bin/neoan3"
    ],
    "autoload": {
        "psr-4": {
            "Creation\\": "Creation",
            "Migration\\": "Migration",
            "Cli\\": "Cli",
            "Set\\": "Set",
            "Helper\\": "Helper"
        }
    },
    "authors": [
        {
            "name": "sroehrl",
            "email": "neoan@neoan.us"
        }
    ],
    "require": {
        "ext-zip": "*",
        "neoan3-apps/template": "^1.0",
        "neoan3-apps/ops": "^2.0",
        "ext-readline": "*",
        "ext-json": "*",
        "neoan3-apps/db": "^0.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
